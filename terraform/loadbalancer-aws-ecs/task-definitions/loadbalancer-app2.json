[
    {
      "name": "cat-loadbalancer",
      "image": "mkorangestripe/loadbalancer:latest",
      "command": ["gunicorn -b 0.0.0.0:80 load_balancer:app"],
      "entryPoint": ["sh", "-c"],
      "essential": true,
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80
        }
      ],
      "memory": 512,
      "cpu": 256
    }
  ]